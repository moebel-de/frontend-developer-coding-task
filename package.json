{
  "name": "moebel-frontend-hr",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-scripts": "3.4.0",
    "styled-components": "^5.1.1",
    "styled-reset": "^4.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^5.1.1",
    "husky": "^4.2.5",
    "jest-styled-components": "^7.0.2",
    "typescript": "~3.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pre-commit"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "src/index.tsx",
      "src/react-app-env.d.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint --ext ts,tsx src --max-warnings 0",
    "test": "react-scripts test -u --watchAll=false",
    "test:watch": "react-scripts test -u",
    "test:coverage": "yarn test --coverage --watchAll=false",
    "test:coverage:watch": "yarn test -- --coverage --watchAll=true",
    "eject": "react-scripts eject",
    "pre-commit": "yarn lint && yarn test:coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}